# GenAI Frontend Configuration

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8080
REACT_APP_WS_URL=ws://localhost:8080/ws

# OpenAI Configuration (if using direct API calls)
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Configuration
REACT_APP_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google AI Configuration
REACT_APP_GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# AWS Configuration
REACT_APP_AWS_REGION=us-east-1
REACT_APP_AWS_ACCESS_KEY_ID=your_aws_access_key_here
REACT_APP_AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# Feature Flags
REACT_APP_ENABLE_FILE_UPLOAD=true
REACT_APP_ENABLE_VOICE_CHAT=false
REACT_APP_ENABLE_IMAGE_GENERATION=true

# Analytics
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Development
REACT_APP_DEBUG_MODE=true
